<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <!-- here we used the tag named property generally every data member is a property here spring will call the setAge method 
    to set the value of age -->
    <bean id="alien" class="com.keepitsimple.SetterInjectionforprimitivetype.Alien">
    <property name="age" value="10"></property>
    
    </bean>


</beans>
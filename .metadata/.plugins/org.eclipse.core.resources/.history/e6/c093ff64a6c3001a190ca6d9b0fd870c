package com.keepitsimple.Autowire;

public class Alien {
	private Computer computer;
	private int age;
	
	public Alien() {
		// TODO Auto-generated constructor stub
		System.out.println("NO argument constructor is called ...");
	}

	public Computer getComputer() {
		return computer;
	}

	public void setComputer(Computer computer) {
		System.out.println("setter is called");
		this.computer = computer;
	}
	
	
	public void getNameOfTheComputer() {
		computer.getName();
	}

	public Alien(int age) {
		super();
		System.out.println("constructor is executing....");
		this.age = age;
		
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		System.out.println("age setter is called .... ");
		this.age = age;
	}

	/* uncomment this for constructor injection autowiring
	 public Alien(Computer computer) {
	super();
	this.computer = computer;
	}
	*/

}
